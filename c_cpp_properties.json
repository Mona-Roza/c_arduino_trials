{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "${default}",
            "compilerPath": "/opt/homebrew/bin/avr-gcc",
            "includePath": [
                "/opt/homebrew/include"
            ],
            "compilerArgs": [
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-fpermissive",
                "-fno-exceptions",
                "-fno-threadsafe-statics",
                "-pipe",
                "-mmcu=ATmega328p",
                "-DF_CPU=8000000UL"
            ]
        }
    ]
}